type CodeSnippet {
    code: String!
    language: Language!
}

enum Difficulty {
    STARTING, EASY, MEDIUM, HARD, GOD
}

enum Language {
    JAVA, SCALA
}

enum SortDirection {
    ASC, DESC
}

input PageInput {
    number: Int = 0
    size: Int = 20
}

type PageOutput {
    number: Int
    size: Int
    totalPages: Int
    totalElements: Int
    hasNext: Boolean
    hasPrevious: Boolean
}